  3.876135800105279*^9}, {3.876135834995262*^9, 3.876135856240663*^9}, {
  3.8762082465716257`*^9, 
  3.876208260415159*^9}},ExpressionUUID->"2bc71a78-a671-4493-975b-\
e00dc7d9ca6f"],

