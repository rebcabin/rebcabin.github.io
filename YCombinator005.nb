Cell[TextData[{
 "We\[CloseCurlyQuote]ve temporarily lost the abstraction of ",
 StyleBox["sf[sf]", "Input"],
 " into ",
 StyleBox["f", "Input"],
 ", but gained a delayed evaluation of ",
 StyleBox["sf[sf]", "Input"],
 ". We\[CloseCurlyQuote]ll get ",
 StyleBox["f", "Input"],
 " back in a minute."
}], "Text",
 CellChangeTimes->{{3.8759811263094587`*^9, 3.8759811497382717`*^9}, {
